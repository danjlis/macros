Universe	= vanilla
Executable 	= scriptForCondor.sh
notification 	= Never
Output  	= condor_$(Process).out
Error		= condor_$(Process).err
Log  		= condor_$(Process).log
Arguments       = "$(Process)"
Initialdir  	= INITDIR
request_memory  = 4.1GB

should_transfer_files = YES
when_to_transfer_output = ON_EXIT

